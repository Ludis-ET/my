{
  "version": 2,
  "builds": [
    {
      "src": "bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "bot.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.jsx"
    }
  ],
  "functions": {
    "api/bot.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
